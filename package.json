{
  "name": "decoded-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified yet\" && exit 0",
    "analyze": "ANALYZE=true next build",
    "complexity": "yarn lint --format json > lint-report.json 2>/dev/null || true && echo 'Lint analysis complete'",
    "typegen:dev": "yarn dlx openapi-typescript-codegen --input https://dev.decoded.style/openapi.json --output ./src/api/generated --client fetch",
    "pre-commit": "yarn typegen:dev && git add src/api/generated/",
    "prepare": "husky"
  },
