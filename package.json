    "@types/matter-js": "^0.19.8",
    "@types/three": "^0.178.1",
    "clsx": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "jose": "^6.0.12",
